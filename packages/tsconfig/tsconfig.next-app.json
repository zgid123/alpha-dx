{
  "extends": "./tsconfig.react.json",
  "compilerOptions": {
    "incremental": true,
    "jsx": "preserve",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "~/*": [
        "./*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "target": "ESNext"
  }
}
